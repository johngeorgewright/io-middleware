{
  "name": "io-middleware",
  "version": "2.0.0",
  "description": "Typed Express middleware for accumulating state across middleware",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "generate-types": "node --loader ts-node/esm bin/generateIOMiddlewareFunction.ts",
    "prepare": "husky install",
    "test": "ts-node-test test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/io-middleware.git"
  },
  "keywords": [
    "express",
    "middleware",
    "state"
  ],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/io-middleware/issues"
  },
  "homepage": "https://github.com/johngeorgewright/io-middleware#readme",
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@commitlint/types": "18.6.1",
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.6",
    "@semantic-release/npm": "11.0.2",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@types/express": "4.17.21",
    "@types/node": "20.16.11",
    "@types/semantic-release": "20.0.6",
    "@types/source-map-support": "0.5.10",
    "@types/supertest": "^6.0.2",
    "commitizen": "4.3.1",
    "concurrently": "8.2.2",
    "cz-conventional-changelog": "3.3.0",
    "express": "4.21.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nodemon": "3.1.7",
    "prettier": "3.2.5",
    "renamer": "5.0.0",
    "rimraf": "5.0.10",
    "semantic-release": "23.0.2",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-node": "10.9.2",
    "ts-node-test": "0.4.3",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@types/express": "^4.0.0",
    "express": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@types/express": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,yml,yaml}": [
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "overrides": {
    "mime@*": "1.6.0"
  }
}
